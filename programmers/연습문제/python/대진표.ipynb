   "source": [
    "def solution(n, a, b):\n",
    "    match = 1\n",
    "    while n > 0:\n",
    "        if abs(a-b) == 1:\n",
    "            if max(a, b) % 2 == 0:\n",
    "                break\n",
    "\n",
    "        match += 1\n",
    "\n",
    "        a = (a+1) // 2\n",
    "        b = (b+1) // 2\n",
    "\n",
    "        n = n // 2\n",
    "    return match"
   ]
